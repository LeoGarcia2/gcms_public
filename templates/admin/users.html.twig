{% extends 'admin/layout.html.twig' %}

{% block gcms_page %}

<h1>Users</h1>

<p><a href="{{ path('admin_register') }}">New user</a></p>

<section id="users">

	{% for user in users %}

		<p><a href="{{ path('admin_register', {id: user.id}) }}">{{ user.username | capitalize }}</a> <a href="{{ path('admin_delete_user', {id: user.id}) }}">Delete</a></p>

	{% endfor %}

</section>

{% endblock %}
